<%= form_for @registered_application do |f| %>
<div class="form-group">
  <%= f.label :name %>
  <%= f.text_field :name, class: 'form-control', placeholder: "Enter app name" %>
</div>
<div class="form-group">
  <%= f.label :url %>
  <%= f.text_field :url, rows: 8, class: 'form-control', placeholder: "Enter app's web address" %>
</div>
<div class="form-group">
  <%= f.submit "Save", class: 'btn btn-success' %>
</div>
<% end %>
