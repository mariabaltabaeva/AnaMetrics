<h1>Edit Registered Application</h1>
<div class="row">
   <div class="col-md-4">
     <p>Guidelines for apps</p>
     <ul>
       <li>Make sure app is working.</li>
       <li>Check spelling twice.</li>
     </ul>
   </div>
 <div class="col-md-8">
   <%= form_for @registered_application do |f| %>
     <div class="form-group">
       <%= f.label :name %>
       <%= f.text_field :name, class: 'form-control', placeholder: "Enter app name" %>
     </div>
     <div class="form-group">
       <%= f.label :url %>
       <%= f.text_field :url, rows: 8, class: 'form-control', placeholder: "Enter post body" %>
     </div>
     <div class="form-group">
       <%= f.submit "Save", class: 'btn btn-success' %>
     </div>
    <% end %>
  </div>
  </div>
